<apex:page Controller="customControllerOfCandidate" >
    <apex:form >
        <apex:pageBlock title="Candidate_AtharvaBhatkhalkar Edit" mode="edit">
            <apex:pageBlockButtons >
                <apex:commandButton action="{!save}" value="Save"/>
                
            </apex:pageBlockButtons>
            <apex:pageBlockSection title="Information" columns="1">
                <apex:inputField value="{!objOfCandidateValues.Name}"/>
                <apex:inputField value="{!objOfCandidateValues.First_Name__c}"/>
                <apex:inputField value="{!objOfCandidateValues.Last_Name__c}"/>
                <apex:inputField value="{!objOfCandidateValues.Salutation__c}"/>
                <apex:inputField value="{!objOfCandidateValues.Date_of_Birth__c}"/>
                <apex:inputField value="{!objOfCandidateValues.Email__c}"/>
                <apex:inputField value="{!objOfCandidateValues.Country__c}"/>
                <apex:inputField value="{!objOfCandidateValues.State__c}"/>
                <apex:inputField value="{!objOfCandidateValues.Application_Date__c}"/>
                <apex:inputField value="{!objOfCandidateValues.Status__c}"/>
                <apex:inputField value="{!objOfCandidateValues.Expected_Salary__c}"/>
